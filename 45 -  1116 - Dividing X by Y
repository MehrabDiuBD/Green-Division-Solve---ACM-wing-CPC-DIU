#include <stdio.h>

int main() {

    int N, X, Y;
    scanf("%d", &N);
    float divisor;

    for(int i=0; i<N; i++) {

        scanf("%d %d", &X, &Y);

          if(Y==0) {
              printf("divisao impossivel\n");
          }
          else {
              divisor = (float)X/(float)Y;
              printf("%.1f\n", divisor);
          }
     }

    return 0;
}
