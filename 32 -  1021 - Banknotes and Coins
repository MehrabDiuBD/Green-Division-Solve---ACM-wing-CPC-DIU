#include <stdio.h>
int main() {

    double N1;
    int n100, n50, n20, n10, n5, n2, n1;
    int N050, N025, N010, N005, N001;

    scanf("%lf", &N1);

    int N = N1 * 100;

    n100 = N / 10000;
    N %= 10000;

    n50 = N / 5000;
    N %= 5000;

    n20 = N / 2000;
    N %= 2000;

    n10 = N / 1000;
    N %= 1000;

    n5 = N / 500;
    N %= 500;

    n2 = N / 200;
    N %= 200;

    n1 = N / 100;
    N %= 100;

    N050 = N / 50;
    N %= 50;

    N025 = N / 25;
    N %= 25;

    N010 = N / 10;
    N %= 10;

    N005 = N / 5;
    N %= 5;

    N001 = N / 1;

    printf("NOTAS:\n");
    printf("%d nota(s) de R$ 100.00\n", n100);
    printf("%d nota(s) de R$ 50.00\n", n50);
    printf("%d nota(s) de R$ 20.00\n", n20);
    printf("%d nota(s) de R$ 10.00\n", n10);
    printf("%d nota(s) de R$ 5.00\n", n5);
    printf("%d nota(s) de R$ 2.00\n", n2);
    printf("MOEDAS:\n");
    printf("%d moeda(s) de R$ 1.00\n", n1);
    printf("%d moeda(s) de R$ 0.50\n", N050);
    printf("%d moeda(s) de R$ 0.25\n", N025);
    printf("%d moeda(s) de R$ 0.10\n", N010);
    printf("%d moeda(s) de R$ 0.05\n", N005);
    printf("%d moeda(s) de R$ 0.01\n", N001);

    return 0;
}
