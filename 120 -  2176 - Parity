// problem link : https://judge.beecrowd.com/en/problems/view/2176

#include<stdio.h>
#include<string.h>

int main () {

    char str[101];
    scanf("%s", str);

    int length = strlen(str);

    int count1=0, i;

    for(i=0; i<length; i++) {if(str[i] == '1') count1++;} //str[i] != '\0'

    if(count1%2==1) {
        
        str[i++] = '1';
        str[i] = '\0';
    }
    
    else {
        
        str[i++] = '0';
        str[i] = '\0';
    }
    
    //printf("%s\n", str);
    puts(str);
    
    return 0;

}
