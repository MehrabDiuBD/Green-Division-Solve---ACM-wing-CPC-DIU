// Problem Link : https://judge.beecrowd.com/en/problems/view/2147

#include <stdio.h>
#include <string.h>
 
int main() {
 
    int n;
    scanf("%d", &n);
    
    for(int i=0; i<n; i++) {
        
        char str[10000];
        scanf("%s", str);
        
        int length = strlen(str);
        
        double time = length/100.0;
        printf("%.2lf\n", time);
    }
 
    return 0;
}
