//Problem Link : https://judge.beecrowd.com/en/problems/view/1557

#include <stdio.h>

int main() {

    int n;
    while(scanf("%d", &n)!=EOF) {

        int x=n;

        for(int i=1; i<=n; i++) {

            for(int j=1; j<=n; j++) {

                  if(x==j) printf("2");
                  else if(i==j) printf("1");
                  else printf("3");
            }

            x--;
            printf("\n");
        }
    }

    return 0;
}
